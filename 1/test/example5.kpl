Program Example2; (* Factorial *)
   
Var _abcd_efgh : Integer;

Function F(_abcd_efgh : Integer) : Integer;
  Begin
    If _abcd_efgh = 0 Then F := 1 Else F := N * F (N - 1);
  End;

Begin
  For _ abcdefgh := 1 To 7 Do
    Begin
      Call WriteLn;
      Call WriteI( F(i));
    End;
End. (* Factorial *)
